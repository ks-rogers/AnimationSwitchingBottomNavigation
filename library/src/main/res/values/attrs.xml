<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="AnimationSwitchingBottomNavigationSelectedButton">
    <attr name="buttonColor" format="color"/>
    <attr name="menuViewId" format="reference"/>
  </declare-styleable>
  <declare-styleable name="AnimationSwitchingBottomNavigationLayout">
    <attr name="navigationViewHeight" format="dimension"/>
    <attr name="selectedButtonSize" format="enum">
      <enum name="normal" value="0"/>
      <enum name="small" value="1"/>
    </attr>
    <attr name="selectedItemLayoutTag" format="string"/>
  </declare-styleable>
  <declare-styleable name="AnimationSwitchingBottomNavigationSelectedItemLayout">
    <attr name="roundedBackgroundColor" format="color"/>
    <attr name="useRoundedBackground" format="boolean"/>
  </declare-styleable>
</resources>